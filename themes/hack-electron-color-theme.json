      "settings": {
        "foreground": "#519449ff"
      }
    },
    {
      "name": "Language Var",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#8FFFE9"
      }
    },
    {
      "name": "Entity Name Function",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#ffaf12"
      }
    },
    {
      "name": "Meta Brace Round",
      "scope": [
